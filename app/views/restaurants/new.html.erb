<div class="container">
  <div class="row">
    <div class="col">
        <h1>Add a restaurant</h1>
            <%= simple_form_for(@restaurant) do |f| %>
                <%= f.input :name %>
                <%= f.input :address %>
                <%= f.input :category, collection: Restaurant::CATEGORIES %>
                <%= f.submit %>
            <% end %>
    </div>
  </div>
</div>