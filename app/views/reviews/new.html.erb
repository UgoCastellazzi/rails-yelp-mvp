<div class="container">
  <div class="row">
    <div class="col">
      <h1>Add a restaurant</h1>
        <%= simple_form_for [@restaurant, @review] do |f| %>
          <%= f.input :content %>
          <%= f.input :rating, collection: (0..5).to_a %>
          <%= f.submit %>
        <% end %>
    </div>
  </div>
</div>